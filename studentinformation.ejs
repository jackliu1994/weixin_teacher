<%- include header %>

    <div class ="container" >
         <div class="row">
		   <table class="table table-striped"
			  
			  <thead>
			  <tr>
			        <th>姓名</th>
				<th>学号</th>
				<th>性别</th>
				<th>年级</th>
				<th>单双证</th>
			 </tr>
			 </thead>
			 <tbody>
			 <% art.forEach(function(student){ %>
			 <tr>
			
				<td>
				    	<a href="/detail/<%=student.stuid%>"><%=student.name %></a>
			    	</td>
			    
			    	<td>    <%=student.stuid %>        </td>
		            	<td>    <%=student.gender %>      </td>
